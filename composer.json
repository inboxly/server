{
    "name": "inboxly/server",
    "type": "project",
    "description": "Inboxly API Server.",
    "keywords": ["inboxly", "server"],
    "license": "MIT",
    "authors": [
        {
            "name": "Sento Sango",
            "email": "sentosango@gmail.com",
            "homepage": "https://github.com/sentosango",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "ext-dom": "*",
        "ext-libxml": "*",
        "debril/feed-io": "^5.0",
        "embed/embed": "^4.3",
        "ezyang/htmlpurifier": "^4.13",
        "fideloper/proxy": "^4.4",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^7.3.0",
        "inboxly/receiver": "^0.1",
        "inboxly/text-processing": "^0.1",
        "kevinrob/guzzle-cache-middleware": "^3.4",
        "laminas/laminas-diactoros": "^2.6",
        "laravel/framework": "^8.54",
        "laravel/tinker": "^2.6"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.10",
        "doctrine/dbal": "^3.1",
        "facade/ignition": "^2.11",
        "fakerphp/faker": "^1.15",
        "mockery/mockery": "^1.4.3",
        "nunomaduro/collision": "^5.8",
        "phpunit/phpunit": "^9.5.6"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "serve": [
            "@php artisan serve"
        ],
        "queue": [
            "@php artisan queue:work"
        ],
        "schedule": [
            "@php artisan schedule:work"
        ],
        "test": "@php artisan test"
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
